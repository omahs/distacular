{
    "compilerOptions": {
        "paths": {
            "*": ["./*.d.ts"]
        },
        "downlevelIteration": true,
        "module": "CommonJS",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "outDir": "lib",
        "declaration": true,
        "resolveJsonModule": true
    }
}
